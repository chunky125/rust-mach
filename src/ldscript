ENTRY(start)

SECTIONS
{
	. = 50M;

	_start = .;

	_text_start = .;	

	.text.boot :
	{
		*(.text.boot)
	}
	.ALIGN = (0x8);

	.text : 
	{ 
		*(.text) 
	}
	.ALIGN = (0x8);
	
	.rodata : 
	{ 
		*(.rodata) 
	}
	.ALIGN = (0x8);
	
	.data : 
	{ 
		*(.data) 
	}
	. = ALIGN(0x8);
	
	_bss_begin = .;
	.bss : 
	{ 
		*(.bss*) 
	} 
	.ALIGN = (0x8);
	_bss_end = .;
	_bss_size = _bss_end - _bss_begin;

	_end = .;
}
