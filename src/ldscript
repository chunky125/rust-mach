ENTRY(start)

SECTIONS
{
	. = 50M;

	_start = .;

	_text_start = .;	

	.text.boot :
	{
		*(.text.boot)
	}
	.ALIGN = (0x8);

	.text : 
	{ 
		*(.text .text.*);
	}
	.ALIGN = (0x8);
	
	.rodata : 
	{ 
		*(.rodata .rodata.*);
	}
	.ALIGN = (0x8);
	
	.data : 
	{ 
		_data_start = .;
		*(.data .data.*);
		_data_end = .;
		KEEP(*(.data .data.*));
	}
	. = ALIGN(0x8);
	
	.bss : 
	{
		_bss_begin = .; 
		*(.bss .bss.*);
		_bss_end = .;
		KEEP(*(.bss .bss.*));	
	} 
	.ALIGN = (0x8);

	_end = .;
}
